<h1>Listing vote_cs</h1>

<table>
  <thead>
    <tr>
      <th>User</th>
      <th>Question</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @vote_cs.each do |vote_c| %>
      <tr>
        <td><%= vote_c.user %></td>
        <td><%= vote_c.question %></td>
        <td><%= link_to 'Show', vote_c %></td>
        <td><%= link_to 'Edit', edit_vote_c_path(vote_c) %></td>
        <td><%= link_to 'Destroy', vote_c, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Vote c', new_vote_c_path %>
