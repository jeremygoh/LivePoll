<h1>Listing vote_bs</h1>

<table>
  <thead>
    <tr>
      <th>User</th>
      <th>Question</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @vote_bs.each do |vote_b| %>
      <tr>
        <td><%= vote_b.user %></td>
        <td><%= vote_b.question %></td>
        <td><%= link_to 'Show', vote_b %></td>
        <td><%= link_to 'Edit', edit_vote_b_path(vote_b) %></td>
        <td><%= link_to 'Destroy', vote_b, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Vote b', new_vote_b_path %>
