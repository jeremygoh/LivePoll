<h1>Listing vote_ds</h1>

<table>
  <thead>
    <tr>
      <th>User</th>
      <th>Question</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @vote_ds.each do |vote_d| %>
      <tr>
        <td><%= vote_d.user %></td>
        <td><%= vote_d.question %></td>
        <td><%= link_to 'Show', vote_d %></td>
        <td><%= link_to 'Edit', edit_vote_d_path(vote_d) %></td>
        <td><%= link_to 'Destroy', vote_d, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Vote d', new_vote_d_path %>
