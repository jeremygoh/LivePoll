<h1>Listing vote_as</h1>

<table>
  <thead>
    <tr>
      <th>User</th>
      <th>Question</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @vote_as.each do |vote_a| %>
      <tr>
        <td><%= vote_a.user %></td>
        <td><%= vote_a.question %></td>
        <td><%= link_to 'Show', vote_a %></td>
        <td><%= link_to 'Edit', edit_vote_a_path(vote_a) %></td>
        <td><%= link_to 'Destroy', vote_a, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Vote a', new_vote_a_path %>
